<prism:PrismApplication x:Class="Resync_Edit.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Resync_Edit"
             xmlns:ui="http://schemas.modernwpf.com/2019"
             xmlns:prism="http://prismlibrary.com/"
             xmlns:convert="clr-namespace:Resync_Edit.Foundation"
             Startup="PrismApplication_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ThemeResources />
                <ui:XamlControlsResources />
            </ResourceDictionary.MergedDictionaries>
            <BooleanToVisibilityConverter x:Key="BoolToVisConverter"></BooleanToVisibilityConverter>
            <convert:TimeSpanToSecondsConverter x:Key="TimeSpanToSecondsConverter"></convert:TimeSpanToSecondsConverter>
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>
