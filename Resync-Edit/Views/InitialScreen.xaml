<UserControl x:Class="Resync_Edit.Views.InitialScreen"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Resync_Edit.Views"
      mc:Ignorable="d" 
      xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
      xmlns:ui="http://schemas.modernwpf.com/2019"
      xmlns:prism="http://prismlibrary.com/"
      d:DesignHeight="450" d:DesignWidth="800" xmlns:viewmodels="clr-namespace:Resync_Edit.ViewModels" d:DataContext="{d:DesignInstance Type=viewmodels:InitialScreen}">
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <WrapPanel>
                <TextBox Width="300"></TextBox>
                <ui:AppBarButton Icon="Folder"></ui:AppBarButton>
            </WrapPanel>
        </StackPanel>
    </Grid>
</UserControl>
